    <body ng-app='myModule' ng-controller="apiController">
        <table>
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>Department</th>
                    <th>Age</th>
                    <th>Username</th>
                    <th>Password</th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat="employee in Employees">
                    <td>{{employee.empId}}</td>
                    <td>{{employee.empName}}</td>
                    <td>{{employee.department}}</td>
                    <td>{{employee.age}}</td>
                    <td>{{employee.userName}}</td>
                    <td>{{employee.password}}</td>
                </tr>
            </tbody>
        </table>
    </body>
